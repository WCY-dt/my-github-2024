{
  "json.schemaDownload.enable": false,
  "files.associations": {
    "my-github-2024": "nginx",
    "*.service": "ini",
    "*.html": "liquid"
  },
  "markdownlint.config": {
    "default": true,
    "MD012": false, // Disable MD012 - Multiple consecutive blank lines
    "MD028": false, // Disable MD028 - Blank line inside block element
    "MD029": false, // Disable MD029 - Ordered list item prefix
    "MD033": false, // Disable MD033 - Inline HTML
    "MD041": false, // Disable MD041 - First line in file should be a top level header
  },
  "[javascript]": {
    "editor.tabSize": 2
  },
  "[css]": {
    "editor.tabSize": 2
  },
  "[html]": {
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.tabSize": 2
  },
  "[liquid]": {
    "editor.tabSize": 2,
    "editor.formatOnSave": false
  },
  "python.formatting.provider": "black",
  "python-envs.pythonProjects": [
    {
      "path": "",
      "envManager": "ms-python.python:system",
      "packageManager": "ms-python.python:pip"
    }
  ]
}